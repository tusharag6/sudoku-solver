{
  "expo": {
    "name": "app",
    "slug": "app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.tusharag6.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "cameraPermission": "The app requires access to your camera for capturing Sudoku puzzles from physical sources, providing a seamless input experience.",
          "photosPermission": "Gallery access is needed to save and load Sudoku puzzles, allowing you to store progress and import puzzles from your device's gallery."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "278041df-fe8e-433a-9cd6-d204662cd4ce"
      }
    }
  }
}
